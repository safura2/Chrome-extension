{
  "manifest_version": 2,
  "name": "WaM",
  "version": "1.0",
  "permissions": [
    "storage", 
    "tabs", 
    "https://web.whatsapp.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
